{
    "name": "instaclick/base-riak-bundle",
    "description": "This bundle provides basic (riak) session support for Symfony2 applications",
    "keywords": ["base", "riak", "session"],
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "InstaClick Inc.",
            "homepage": "http://www.instaclick.com"
        }
    ],
    "require": {
        "php": ">=5.3.2",
        "ext-riak": ">=0.5",
        "symfony/symfony": "2.4.*",
        "ocramius/proxy-manager": "0.5.*"
    },
    "require-dev": {
        "phpunit/PHPUnit": "3.7.*",
        "instaclick/base-test-bundle": "dev-master"
    },
    "autoload": {
        "psr-0": { "IC\\Bundle\\Base\\RiakBundle": "" }
    },
    "target-dir": "IC/Bundle/Base/RiakBundle",
    "minimum-stability": "dev"
}
